{
  "name": "@dil-ddaradics/ai-sandbox-mcp-server",
  "version": "0.1.0",
  "description": "Model Context Protocol server for AI Sandbox",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "ai-sandbox-mcp": "./bin/cli.js"
  },
  "scripts": {
    "build": "tsc && npm run build:cli",
    "build:cli": "chmod +x ./bin/cli.js",
    "start": "ts-node --esm src/index.ts",
    "dev": "ts-node-dev --respawn --esm src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build",
    "publish": "npm publish",
    "version": "npm version"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dil-ddaradics/ai-sandbox.git",
    "directory": "mcp-server"
  },
  "keywords": [
    "mcp",
    "ai",
    "claude",
    "model-context-protocol",
    "ai-sandbox"
  ],
  "author": "dil-ddaradics",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/",
    "bin/",
    "README.md",
    "LICENSE"
  ]
}